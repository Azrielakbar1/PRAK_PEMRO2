<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="ancDataPelanggan" prefHeight="400.0" prefWidth="600.0" 
            xmlns="http://javafx.com/javafx/21" 
            xmlns:fx="http://javafx.com/fxml/1" 
            fx:controller="controller.PelangganController">
   <children>
      <TableView fx:id="tabelPelanggan" layoutY="176.0" prefHeight="197.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="kolNamaPelanggan" prefWidth="75.0" text="Nama" />
          <TableColumn fx:id="kolEmailPelanggan" prefWidth="150.0" text="Email" />
          <TableColumn fx:id="kolTeleponPelanggan" prefWidth="100.0" text="Telepon" />
        </columns>
      </TableView>
      <TextField fx:id="txtNamaPelanggan" layoutX="81.0" layoutY="14.0" />
      <Button fx:id="btnTambahPelanggan" layoutX="32.0" layoutY="141.0" mnemonicParsing="false" onAction="#aksiTambahPelanggan" prefHeight="15.0" prefWidth="42.0" text="Add" />
      <TextField fx:id="txtEmailPelanggan" layoutX="81.0" layoutY="55.0" />
      <TextField fx:id="txtTeleponPelanggan" layoutX="81.0" layoutY="99.0" />
      <Label fx:id="lbNamaPelanggan" layoutX="25.0" layoutY="18.0" text="Nama" />
      <Label fx:id="lbEmailPelanggan" layoutX="25.0" layoutY="59.0" text="Email" />
      <Label fx:id="lbTeleponPelanggan" layoutX="25.0" layoutY="103.0" text="Telepon" />
      <Button fx:id="btnEditPelanggan" layoutX="83.0" layoutY="141.0" mnemonicParsing="false" onAction="#aksiEditPelanggan" prefHeight="15.0" prefWidth="42.0" text="Edit" />
      <Button fx:id="btnHapusPelanggan" layoutX="134.0" layoutY="141.0" mnemonicParsing="false" onAction="#aksiHapusPelanggan" prefHeight="26.0" prefWidth="51.0" text="Delete" />
   </children>
</AnchorPane>